package com.experion.entity;

import java.util.ArrayList;

public class Accounts extends Product {
	private String accountType;
	ArrayList<BankService>bankService=new ArrayList<BankService>();
	public Accounts(String productCode, String productName, String prductType, String accountType,
			ArrayList<BankService> bankService) {
		super(productCode, productName, prductType);
		this.accountType = accountType;
		this.bankService = bankService;
	}
	public String getAccountType() {
		return accountType;
	}
	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}
	public ArrayList<BankService> getBankService() {
		return bankService;
	}
	public void setBankService(ArrayList<BankService> bankService) {
		this.bankService = bankService;
	}
	
	

}
